{
  "name": "PerceptiLabs",
  "version": "0.10.0",
  "author": "PerceptiLabs <contact@perceptilabs.com>",
  "description": "PerceptiLabs is a cross-platform software that will make it easier to use Machine Learning, especially Deep Learning.",
  "homepage": "perceptilabs.com",
  "bugs": {
    "url": "https://join.slack.com/t/perceptilabs-com/shared_invite/enQtODQ5NzAwNDkxOTExLWUxODAwZDk0MzA1MmM4OTViNWE4MmVjYjc2OTQwMTQ4N2NmM2ZlYmI5NjZjOWRiYjBkYjBjMTMzNjEyMDNiNDk"
  },
  "license": "UNLICENSED",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 node .electron-vue/build.js && electron-builder",
    "build:debug": "cross-env IS_DEBUG_MODE=true NODE_OPTIONS=--max_old_space_size=8192 node .electron-vue/build.js && electron-builder",
    "publish": "electron-builder -p always",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env NODE_OPTIONS=--max_old_space_size=8192 BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "dev:web": "webpack-dev-server --inline --progress --config src/build/webpack.dev.conf.js",
    "start:web": "npm run dev:web",
    "build-render": "node src/build/build.js",
    "dev:all": "npm run dev & npm run dev:web"
  },
  "build": {
    "productName": "PerceptiLabs",
    "appId": "com.perceptilabs.app",
    "directories": {
      "buildResources": "build/icons/icon.*",
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraFiles": [
      "core",
      "basic-data"
    ],
    "publish": [
      {
        "provider": "generic",
        "url": "https://uantumetdisks.blob.core.windows.net/updates-admin"
      }
    ],
    "dmg": {
      "sign": false,
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "category": "public.app-category.productivity",
      "hardenedRuntime": true,
      "entitlements": "build/entitlements.mac.inherit.plist",
      "gatekeeperAssess": false,
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        "nsis"
      ],
      "verifyUpdateCodeSignature": false
    },
    "linux": {
      "icon": "build/icons",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "@sentry/browser": "^5.9.1",
    "@sentry/integrations": "^5.8.0",
    "axios": "^0.18.1",
    "canvg": "^1.5.3",
    "codemirror": "^5.49.0",
    "echarts-gl": "^1.1.1",
    "electron-updater": "^4.1.2",
    "html2canvas": "^1.0.0-rc.4",
    "lodash.clonedeep": "4.5.0",
    "mixpanel-browser": "^2.29.1",
    "moveable": "0.15.2",
    "showdown": "^1.9.1",
    "universal-analytics": "^0.4.20",
    "v-hotkey": "^0.6.0",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.10",
    "vue-color": "^2.7.0",
    "vue-cropper": "^0.4.9",
    "vue-echarts": "^3.1.3",
    "vue-electron": "^1.0.6",
    "vue-nonreactive": "^0.1.0",
    "vue-router": "^3.1.3",
    "vue-the-mask": "^0.11.1",
    "vue2-perfect-scrollbar": "1.3.0",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/preset-stage-0": "^7.0.0",
    "ajv": "^6.10.2",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.1",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-notarize": "^0.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^7.3.1",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^1.1.2",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.2.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "14.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.40.2",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
