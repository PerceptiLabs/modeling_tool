.workspace_scale()
  button.btn.btn--icon.workspace-scale_btn(type="button" @click="decScale()")
    i.icon.icon-minus

  .input-wrap_icon.workspace-scale_input
    input(type="text" v-model.number="scaleNet")
    span %

  button.btn.btn--icon.workspace-scale_btn(type="button" @click="incScale()")
    i.icon.icon-zoom-plus

  //base-checkbox(:icon-theme="true") Map
.workspace_meta-action
  //-button.btn.btn--color.btn--color__purpur(type="button" disabled="disabled"
    v-if="statisticsIsOpen && statusNetworkCore == 'Finished'"
    @click="saveNetwork"
    ) Extract Data
